[user]
	email = tommy.deudaeta@gmail.com
	[credential "https://github.com"]
	helper =
	helper = !/usr/bin/gh auth git-credential

[color "status"]
	untracked = red bold
	changed = 219 bold
	deleted = 232 strike # negro
	unmerged = 196 bold # rojo power
	staged = blue bold
	updated = 205 bold
	unstaged = 240 # gris
#	unchanged = red bold
#	nobranch = magenta bold
	header = yellow
[color "diff"]
	old = 009 strike # rojo
	meta = 219 bold
	plain = white
	frag = 219 # rosa
	new = 157 # lima clarito
[color]
	ui = always
	default = red bold
#	ui = true
[color "branch"]
	remote = 092 # violetita cute
	current = green # verde claro
	plain = magenta bold
	local = 028 # verde oscuro
[color "interactive"]
	error = normal
	header = red bold
	help = normal
	prompt = red bold
[color "decorate"]
	HEAD = cyan bold
	branch = green bold
	#tag = magenta bold
[pager]
	branch = false
	log = false
	diff = false
[alias]
	graph = "log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold 192)%as%C(reset) %C(bold 190)(%ar)%C(reset)%C(auto)%d%C(reset)%n''          %C(013)%s%C(reset) %C(dim white)- %an%C(reset)' --all"
[log]
	graphcolors = 154, 196, 190, 021, 201, cyan, bold 190, bold 196, bold 154, bold 021, bold 201, bold cyan
[init]
	defaultBranch = main
